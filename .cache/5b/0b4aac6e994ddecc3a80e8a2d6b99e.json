sform: uppercase;\n  cursor: pointer;\n  position: relative;\n}\n\n/* .list__item.active {\n  color: #fff;\n} */\n\n.list__item.active::after {\n  content: '';\n  position: absolute;\n  bottom: -1.5rem;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 100%;\n  height: .3rem;\n  background-color: #fff;\n}\n\n.list__item:not(.active):hover::after {\n  content: '';\n  position: absolute;\n  bottom: -1.5rem;\n  left: 0;\n  transform: translateX(-50%);\n  width: 0;\n  height: .3rem;\n  background-color:rgba(255, 255, 255, 0.5); /* Grey highlight color */\n  transition: width,left 1s ease-in-out;\n}\n\n.list__item:not(.active):hover::after{\n  left: 50%;\n  width: 100%;\n}\n\n\n.planet-name {\n  font-weight: 400;\n  margin: -1rem 1rem;\n  \n}\n\n/* Planet description */\n.planet-description {\n  line-height: 1.8;\n  margin: 0 1.5rem 1rem;\n  padding: 1rem;\n  /* display: flex; */\n  text-align: center;\n}\n\n/* Stats */\n.planet-stats {\n  /* display: flex; */\n  justify-items: center;\n  margin-top: 2.5rem;\n  /* width: 100%; */\n}\n\n.stat{\n    text-align: center;\n}\n\n.stat p {\n  font-size: 1.5rem;\n  font-family: Barlow Condensed;\n  font-weight: 400;\n  line-height: 16.8px;\n  letter-spacing: 2px;\n  text-transform: uppercase;\n  /* margin-top: 1rem; */\n}\n\n.stat__num{\n  margin-top: -1.5rem;\n  margin-bottom: 2rem;\n  font-size:2.8rem;\n  /* line-height: 40px; */\n}\n\n.planet__separator{\n  height: 1.5px;\n  /* width: 80%; */\n  /* max-width: 500px; */\n  background-color: rgba(255, 255, 255, 0.5);\n  /* margin: 2rem; */\n  margin:0 2.5rem;\n  opacity: 0.5;\n}\n\n@media screen and (min-width:768px) {\n  /* .main--content{\n    display: flex;\n    flex-direction: column;\n    justify-items: center;\n    padding: 1rem;\n  } */\n    .tagline{\n        justify-content: left;\n        margin-left: 2rem;\n        padding: 2rem;\n    }\n    .planet__image{\n        width: 30rem;\n    }\n    .planet-description{\n      padding: 0 8rem;\n    }\n    .col2{\n      padding: 1rem;\n    }\n    .planet-stats{\n        display: flex; /* Enable Flexbox */\n  justify-content: space-around; /* Push items to opposite ends */\n  margin: 0 auto; \n  padding: 0 3rem; /* Add padding for space inside the container */\n  max-width: 100%; \n  flex-wrap: nowrap; /* Prevent wrapping of items to the next line */\n  overflow: hidden; /* Prevent any overflow of content */\n  box-sizing: border-box; \n        \n    }\n    .stat{\n       padding: 2rem;\n       text-align: center; \n       flex: 0 0 auto;\n    }\n    .stat p{\n      font-size: 1.8rem;\n    }\n    .stat__num{\n      font-size: 3rem;\n    }\n    .planet__separator{\n      width: 80%;\n      justify-self: center;\n      margin-top: 2.5rem;\n    }\n}\n\n@media screen and (min-width:1440px) {\n  .planet_page{\n    margin-left: 10rem;\n  }\n  .main--content{\n    /* justify-items: center; */\n    padding: 4rem 10rem;\n  }\n    .tagline{\n        justify-content: left;\n        margin-left: 0;\n        padding: 2rem;\n    }\n    .planet__image{\n        width: 58rem;\n        /* height: auto; */\n        /* object-fit:c; */\n    }\n    .grid--1{\n    display: grid;\n    grid-template-columns: 1fr 1fr; /* Two equal columns */\n    gap: 6rem; \n    align-items: start; \n    justify-content: center;\n    padding: 4rem 5rem;\n    height:0%;\n    justify-items: left;\n    /* max-width: 1440px; */\n    width: 100%;\n    margin: 0 auto; \n    }\n    .col1{\n      display: flex;\n  justify-content: center; /* Center-align the image horizontally */\n  align-items: start;\n    }\n    .col2{\n       display: flex;\n  flex-direction: column; /* Stack content vertically */\n  justify-content: flex-start; /* Align content to the top */\n  align-items: flex-start;\n    }\n    .planet__list{\n      margin-top: 0;\n    }\n    .planet-description{\n      text-align: left;\n      margin: 0;\n      padding: 0 4rem 0 0;\n    }\n    .planet__separator{\n      width: 100%;\n      /* margin: 0; */\n      margin: 5rem 0;\n    }\n    .planet-stats, .stat{\n      padding: 0;\n      margin-left: 0;\n      text-align: left;\n      gap: 8rem;\n    }\n    .planet-stats{\n      display: flex; /* Enable Flexbox */\n  justify-content: space-between; /* Push items to opposite ends */\n  max-width: 100%; \n  flex-wrap:nowrap; /* Prevent wrapping of items to the next line */\n  overflow: hidden; /* Prevent any overflow of content */\n  box-sizing: border-box; \n  margin: 0;\n  padding: 0;\n    }\n    .stat p{\n      margin-top: 0;\n    }\n    .stat__num{\n      font-size: 4.2rem;\n    }\n}"},"lineCount":null}},"error":null,"hash":"17744b5b21f31d9efdf059de3e1b5586","cacheData":{"env":{}}}